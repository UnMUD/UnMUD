$(srcdir)/package.m4: $(top_srcdir)/configure.ac $(srcdir)/Makefile.am
	$(AM_V_GEN){                                      \
	  echo '# Signature of the current package.'; \
	  echo 'm4_define([AT_PACKAGE_NAME],      [gdbm])'; \
	  echo 'm4_define([AT_PACKAGE_TARNAME],   [gdbm])'; \
	  echo 'm4_define([AT_PACKAGE_VERSION],   [1.23])'; \
	  echo 'm4_define([AT_PACKAGE_VERSION_MAJOR], [1])'; \
	  echo 'm4_define([AT_PACKAGE_VERSION_MINOR], [23])'; \
	  echo 'm4_define([AT_PACKAGE_VERSION_PATCH], [0])'; \
	  echo 'm4_define([AT_PACKAGE_STRING],    [gdbm 1.23])'; \
	  echo 'm4_define([AT_PACKAGE_BUGREPORT], [bug-gdbm@gnu.org])'; \
	} >$(srcdir)/package.m4
$(TESTSUITE): package.m4 $(TESTSUITE_AT)
	$(AUTOTEST) -I $(srcdir) testsuite.at -o $@.tmp
	mv $@.tmp $@

atconfig: $(top_builddir)/config.status
	cd $(top_builddir) && ./config.status tests/$@

